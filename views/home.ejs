<%- include("partials/header") -%>

<h1>Home</h1>
<p><%= embed_content_home %></p>

<% embed_posts.forEach(function(post){ %>
  <h2><%= post.title %></h2>
  <p>
    <%= post.content.substring(0, 100) + "..." %>
    <a href="/posts/<%= post.title %>">Read More</a>
  </p>

<% }); %>

<%- include("partials/footer") -%>
